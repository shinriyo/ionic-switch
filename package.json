{
    "name": "ionic-switch",
    "displayName": "Ionic Switch",
    "description": "Switch .html and .ts for Ionic",
    "icon": "images/ionic-switch.png",
    "version": "0.0.1",
    "publisher": "shinriyo",
    "repository": {
      "type": "git",
      "url": "git://github.com/shinriyo/ionic-switch.git"
    },
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.ionSwitchHTML",
        "onCommand:extension.ionSwitchTS"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.ionSwitchHTML",
                "title": "Switch .html file"
            },
            {
                "command": "extension.ionSwitchTS",
                "title": "Switch .ts file"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
